    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "agora-chat": "^1.3.1",
    "agora-rtc-sdk-ng": "^4.20.0",
    "agora-rtm-sdk": "^1.5.1",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
