{
  "name": "@digis/frontend",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true vite build",
    "build:optimize": "vite build --config vite.config.optimized.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "build:css": "tailwindcss -i ./src/index.css -o ./src/output.css --minify",
    "dev:css": "tailwindcss -i ./src/index.css -o ./src/output.css --watch",
    "analyze": "node analyze-bundle.js",
    "clean": "rm -rf node_modules pnpm-lock.yaml dist && pnpm install"
  },
  "dependencies": {
