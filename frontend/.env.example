# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Backend Configuration
VITE_BACKEND_URL=http://localhost:3005

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-publishable-key

# Agora Configuration (optional - usually handled by backend)
VITE_AGORA_APP_ID=your_agora_app_id_here
VITE_AGORA_RTC_VERSION=4.20.0
VITE_AGORA_RTM_VERSION=1.5.1
VITE_AGORA_EXTENSIONS_VERSION=4.20.0

# Agora Chat Configuration
VITE_AGORA_CHAT_APP_KEY=your_org_name#your_app_name
VITE_AGORA_CHAT_ORG_NAME=your_org_name
VITE_AGORA_CHAT_APP_NAME=your_app_name

# Feature Flags
VITE_ANALYTICS_ENABLED=true
VITE_PWA_ENABLED=true
VITE_NOTIFICATIONS_ENABLED=true
VITE_MOBILE_ONBOARDING_ENABLED=false

# Sentry Error Tracking (optional)
# Get your DSN from https://sentry.io/settings/[org]/projects/[project]/keys/
# This is your digis-frontend project DSN
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_SENTRY_ENABLED=true
VITE_APP_VERSION=1.0.0
VITE_USE_SUPABASE_STORAGE=true

# App Configuration
VITE_APP_NAME=Digis
VITE_APP_VERSION=2.0.0